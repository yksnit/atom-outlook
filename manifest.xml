      <bt:LongStrings>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="ATOM Aviation Deal support add-in(QA)"/>
      </bt:LongStrings>
    </Resources>
    
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1"
