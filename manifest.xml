  <Version>1.0.0.0</Version>
  <ProviderName>ATOM</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Sample - ATOM Aviation Deals(QA)"/>
  <Description DefaultValue="ATOM Insurnace MS Outlook add-ins for Deals"/>
  <IconUrl DefaultValue="https://yksnit.github.io/atom-outlook/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://yksnit.github.io/atom-outlook/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://www.atom-insurance.com"/>
  <AppDomains>
    <AppDomain>https://www.atom-insurance.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
