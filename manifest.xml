                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://yksnit.github.io/atom-outlook/assets/icon-16.png"/>
          <bt:Image id="Icon.32x32" DefaultValue="https://yksnit.github.io/atom-outlook/assets/icon-32.png"/>
          <bt:Image id="Icon.80x80" DefaultValue="https://yksnit.github.io/atom-outlook/assets/icon-80.png"/>
